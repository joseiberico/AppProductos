@{
    ViewData["Title"] = "Usuario";
}

@model List<AppWebProductos.Models.Usuario>


<h1 class="display-4">Tabla de Usuarios</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Email</th>
            <th>Contraseña</th>
        </tr>
    </thead>
    <tbody>
           @foreach(var usuario in Model)
         {
            <tr>    
                    <td>@usuario.Id_Usuario </td>
                    <td>@usuario.Nombre </td>
                    <td>@usuario.Email </td>
                    <td>@usuario.Contrasena </td>
            </tr>
         }
        
     </tbody>
</table>